generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id        String    @id @default(uuid())
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  name      String
  email     String
  messanger Messanger @default(Telegram)
  userName  String
  message   String
}

enum Messanger {
  Telegram
  Whatsapp
  Instagram
}
